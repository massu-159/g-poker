{
  "name": "g-poker",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "prebuild": "expo prebuild --clean",
    "build": "./scripts/build.sh",
    "build:ios": "./scripts/build.sh preview ios",
    "build:android": "./scripts/build.sh preview android",
    "build:production": "./scripts/build.sh production all",
    "build:development": "./scripts/build.sh development all",
    "deploy:ios": "eas submit --platform ios",
    "deploy:android": "eas submit --platform android",
    "deploy": "eas submit --platform all",
    "update": "eas update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/massu-159/g-poker.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/massu-159/g-poker/issues"
  },
  "homepage": "https://github.com/massu-159/g-poker#readme",
  "description": "",
  "dependencies": {
    "@expo/metro-runtime": "~6.1.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@shopify/restyle": "^2.4.5",
    "@supabase/supabase-js": "^2.57.2",
    "@tanstack/react-query": "^5.87.1",
    "expo": "~54.0.2",
    "expo-apple-authentication": "~8.0.7",
    "expo-auth-session": "~7.0.8",
    "expo-crypto": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~19.1.0",
    "@types/react-native": "^0.72.8",
    "eslint": "^8.57.1",
    "jest": "^29.4.0",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ]
  }
}
