    "@shopify/restyle": "^2.4.5",
    "@supabase/supabase-js": "^2.57.2",
    "@tanstack/react-query": "^5.87.1",
    "expo": "~53.0.22",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
